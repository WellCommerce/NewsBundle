<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <imports>
        <import resource="admin/news.xml"/>
        <import resource="front/news.xml"/>
    </imports>

    <services>
        <service id="news.manager" class="WellCommerce\Bundle\NewsBundle\Manager\NewsManager" parent="core.manager">
            <argument type="service" id="news.repository"/>
            <argument type="service" id="news.factory"/>
        </service>
    </services>
</container>
